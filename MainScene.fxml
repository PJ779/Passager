<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.ui.MainSceneController">
    <children>
        <ToolBar prefHeight="40.0">
            <items>
                <TextField fx:id="searchField" onAction="#onSearchFieldUsed" prefHeight="26.0" promptText="Filter..." HBox.hgrow="SOMETIMES" />
                <HBox alignment="CENTER_RIGHT" spacing="8.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#onAddAccountButtonClicked" prefHeight="26.0" prefWidth="110.0" text="Add Account" />
                        <Button mnemonicParsing="false" onAction="#onLogoutButtonClicked" text="Logout" />
                    </children>
                </HBox>
            </items>
        </ToolBar>
      <HBox prefHeight="384.0" prefWidth="600.0">
         <children>
            <ScrollPane prefHeight="106.0" prefWidth="266.0">
              <content>
                  <ListView fx:id="listView" prefHeight="3010.0" />
              </content>
            </ScrollPane>
            <VBox prefHeight="364.0" prefWidth="334.0" spacing="5.0">
               <children>
                  <HBox prefHeight="32.0" prefWidth="216.0">
                     <children>
                        <Label alignment="CENTER" prefHeight="24.0" prefWidth="80.0" text="Website/App" />
                        <TextField fx:id="websiteField" prefHeight="25.0" prefWidth="229.0" />
                     </children>
                  </HBox>
                  <HBox layoutX="10.0" layoutY="10.0" prefHeight="32.0" prefWidth="216.0">
                     <children>
                        <Label alignment="CENTER" prefHeight="24.0" prefWidth="80.0" text="Username" />
                        <TextField fx:id="usernameField" prefHeight="25.0" prefWidth="229.0" />
                     </children>
                  </HBox>
                  <HBox layoutX="10.0" layoutY="42.0" prefHeight="32.0" prefWidth="216.0">
                     <children>
                        <Label alignment="CENTER" prefHeight="24.0" prefWidth="80.0" text="Email" />
                        <TextField fx:id="emailField" prefHeight="25.0" prefWidth="229.0" />
                     </children>
                  </HBox>
                  <HBox layoutX="10.0" layoutY="74.0" prefHeight="32.0" prefWidth="216.0">
                     <children>
                        <Label alignment="CENTER" prefHeight="24.0" prefWidth="80.0" text="Password" />
                        <TextField fx:id="passwordField" prefHeight="25.0" prefWidth="229.0" />
                     </children>
                  </HBox>
                  <HBox layoutX="10.0" layoutY="106.0" prefHeight="190.0" prefWidth="334.0">
                     <children>
                        <Label alignment="CENTER" prefHeight="24.0" prefWidth="80.0" text="Expires" />
                        <TextField fx:id="expireField" prefHeight="25.0" prefWidth="229.0" />
                     </children>
                  </HBox>
                  <HBox layoutX="10.0" layoutY="106.0" prefHeight="40.0" prefWidth="334.0" spacing="10.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#onCopyPasswordButtonClicked" prefHeight="25.0" prefWidth="120.0" text="Copy Password" />
                        <Button mnemonicParsing="false" onAction="#onEditButtonClicked" prefHeight="25.0" prefWidth="100.0" text="Edit" />
                        <Button mnemonicParsing="false" onAction="#onDeleteButtonClicked" prefHeight="25.0" prefWidth="70.0" text="Delete" />
                     </children>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="10.0" />
                     </padding>
                  </HBox>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </VBox>
         </children>
      </HBox>
    </children>
</VBox>
